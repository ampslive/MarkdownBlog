{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api",
      "cors": {
        "allowedOrigins": [
          "http://localhost:3000/",
          "https://www.amitphilips.com"
        ],
        "allowedMethods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ],
        "allowedHeaders": [
          "Content-Type",
          "Authorization"
        ]
      }
    }
  }
}